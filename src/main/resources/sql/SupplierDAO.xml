<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.varchar.www.model.dao.SupplierDAO">
	
	<select id="getSupplier" resultType="com.varchar.www.model.domain.manager.Supplier">
		SELECT * FROM supplier ORDER BY supplier_no DESC
	</select>
	
	<select id="searchSupplier" resultType="com.varchar.www.model.domain.manager.Supplier">
		SELECT * FROM supplier WHERE #{filter} LIKE %#{searchSupplier}% DESC;
	</select>
		
	
</mapper>